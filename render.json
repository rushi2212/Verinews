{
  "buildCommand": "cd backend && pip install -r requirements.txt",
  "devCommand": "npm run dev --prefix frontend",
  "envVars": [
    {
      "key": "GOOGLE_API_KEY",
      "sync": false
    },
    {
      "key": "TAVILY_API_KEY",
      "sync": false
    },
    {
      "key": "GEMINI_MODEL",
      "value": "gemini-2.5-flash"
    }
  ]
}
